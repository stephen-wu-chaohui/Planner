@using Microsoft.AspNetCore.Components.Web
@using Microsoft.Extensions.Configuration
@inject IConfiguration Config

@{
    var interactive = new InteractiveServerRenderMode(prerender: false);
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <base href="/" />
    <link rel="stylesheet" href="bootstrap/bootstrap.min.css" />
    <link rel="stylesheet" href="app.css" />
    <link rel="stylesheet" href="Planner.BlazorApp.styles.css" />
    <link rel="stylesheet" href="css/planner-entities-modal.css" />
    <link rel="stylesheet" href="css/polished-tables.css" />
    <link rel="icon" type="image/png" href="favicon.png" />
    <HeadOutlet @rendermode="interactive" />
</head>

<body>
    <Routes @rendermode="interactive" />

    <script src="js/plannerMap.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=@(Config["GoogleMaps:ApiKey"])&libraries=marker,places&callback=mapInteropInit" async defer></script>
    <script src="_framework/blazor.web.js"></script>
</body>

</html>
